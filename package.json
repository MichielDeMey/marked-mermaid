{
  "name": "marked-mermaid",
  "version": "0.1.0",
  "description": "Marked.js extensions to render Mermaid.js",
  "main": "./lib/index.cjs",
  "module": "./src/index.js",
  "browser": "./lib/index.umd.js",
  "type": "module",
  "keywords": [
    "marked",
    "extension"
  ],
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "test": "jest --verbose",
    "test:cover": "jest --coverage",
    "lint": "eslint .",
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichielDeMey/marked-mermaid.git"
  },
  "author": "Michiel De Mey <michiel@spaceowl.be> (https://spaceowl.be)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichielDeMey/marked-mermaid/issues"
  },
  "homepage": "https://github.com/MichielDeMey/marked-mermaid#readme",
  "peerDependencies": {
    "marked": "^4.2.12"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "babel-jest": "^29.4.3",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest-cli": "^29.4.3",
    "marked": "^4.2.12",
    "rollup": "^3.17.3",
    "semantic-release": "^20.1.0"
  },
  "dependencies": {
    "lodash.memoize": "^4.1.2",
    "mermaid": "^10.0.2"
  }
}
